<ul>
    <li><%= link_to "Home", root_path %></li>
    <%if current_user != nil && current_user.role != 1 %>
        <li><%= link_to "Organize", "/teamchart"%></li>
        <li><%= link_to "Discipline", "/discipline"%></li>
        <li><%= link_to "Attendance", "/attendance"%></li>
        <li><%= link_to "Coaching", "/coaching"%></li>
        <%if current_user.role == 0%>
            <li><%= link_to "All Agents", myTeam_path%></li>
        <%else%>
            <li><%= link_to "My Agents", myTeam_path%></li>
        <%end%>
    <% end %>
    <li>Documents</li>
    <%if current_user != nil%>    
        <li style="float: right;"><%= image_tag("rank#{@current_user.certLevel}.png", :alt => "rss feed", class: "rank") %></li> 
        <li style="float: right; text-decoration: underline;"><%= link_to @current_user.name, "/users/#{@current_user.id}"%></li>
        <li style="float: right;"><%= link_to "Logout", logout_path, method: :delete%></li>
    <%else%>
        <li style="float: right;"><%= link_to "Login", login_path%></li>
    <%end%>
</ul>